    REFERENCE
        "DOCSIS Baseline Privacy Plus Interface Specification,
        Section 4.2.2.12."
    ::= { docsBpi2CmIpMulticastMapEntry 4 }

docsBpi2CmIpMulticastSAMapState OBJECT-TYPE
    SYNTAX INTEGER {
        start(1),
        mapWait(2),
        mapped(3)
        }
