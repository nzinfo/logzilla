docsBpi2CmAuthRejectErrorCode OBJECT-TYPE
    SYNTAX INTEGER {
        none(1),
        unknown(2),
        unauthorizedCm(3),
        unauthorizedSaid(4),
        permanentAuthorizationFailure(8),
        timeOfDayNotAcquired(11)
        }
    ACCESS read-only
